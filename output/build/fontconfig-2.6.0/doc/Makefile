	for dir in "$(DESTDIR)$(man3dir)" "$(DESTDIR)$(man5dir)" "$(DESTDIR)$(docdir)" "$(DESTDIR)$(htmldocdir)"; do \
	  test -z "$$dir" || $(MKDIR_P) "$$dir"; \
	done
