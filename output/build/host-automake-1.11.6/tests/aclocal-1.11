#!/bin/sh
# tests/aclocal-1.11.  Generated from aclocal.in by configure.

# Be Bourne compatible
if test -n "${ZSH_VERSION+set}" && (emulate sh) >/dev/null 2>&1; then
  emulate sh
  NULLCMD=:
  # Zsh 3.x and 4.x performs word splitting on ${1+"$@"}, which
  # is contrary to our usage.  Disable this feature.
  alias -g '${1+"$@"}'='"$@"'
fi

perllibdir="/home/simon/GIT/buildroot-k3-current/output/build/host-automake-1.11.6/lib:/home/simon/GIT/buildroot-k3-current/output/build/host-automake-1.11.6/lib"
export perllibdir

exec "/home/simon/GIT/buildroot-k3-current/output/build/host-automake-1.11.6/aclocal" "--automake-acdir=/home/simon/GIT/buildroot-k3-current/output/build/host-automake-1.11.6/m4" \
     "--system-acdir=/home/simon/GIT/buildroot-k3-current/output/build/host-automake-1.11.6/m4/acdir" ${1+"$@"}
