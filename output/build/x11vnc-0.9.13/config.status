t clr
: clr
${ac_dA}PACKAGE_NAME${ac_dB}PACKAGE_NAME${ac_dC}"x11vnc"${ac_dD}
${ac_dA}PACKAGE_TARNAME${ac_dB}PACKAGE_TARNAME${ac_dC}"x11vnc"${ac_dD}
${ac_dA}PACKAGE_VERSION${ac_dB}PACKAGE_VERSION${ac_dC}"0.9.13"${ac_dD}
${ac_dA}PACKAGE_STRING${ac_dB}PACKAGE_STRING${ac_dC}"x11vnc 0.9.13"${ac_dD}
${ac_dA}PACKAGE_BUGREPORT${ac_dB}PACKAGE_BUGREPORT${ac_dC}"http://sourceforge.net/projects/libvncserver"${ac_dD}
${ac_dA}PACKAGE${ac_dB}PACKAGE${ac_dC}"x11vnc"${ac_dD}
${ac_dA}VERSION${ac_dB}VERSION${ac_dC}"0.9.13"${ac_dD}
${ac_dA}ALLOW24BPP${ac_dB}ALLOW24BPP${ac_dC}1${ac_dD}
${ac_dA}STDC_HEADERS${ac_dB}STDC_HEADERS${ac_dC}1${ac_dD}
